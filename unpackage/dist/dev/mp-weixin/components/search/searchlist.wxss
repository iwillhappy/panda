.search-list{
	width: 100%;
	height: calc(100% - 281rpx );
	
	position: absolute;
	left: 0;
	top: 281rpx;
	
	
	
	height: calc(100% - 102rpx);
	top:102rpx;
	
	background: #FFFFFF;
}
.fixedEle{
	width: 100%;
	height: 20rpx;
	background: #f4f7fd;
	
	margin-top: 20rpx;
	
}
.scroll-box{
	width: 100%;
	height: calc(100% - 20rpx);
	background: #FFFFFF;
}
.list-item{
	width: calc(100% - 80rpx);
	height: 80rpx;
	line-height:80rpx;
	border-bottom: 1px solid #f4f7fd;
	padding: 0 40rpx;
	font-size: 14px;
	color: #333333;
	list-style: none;
	letter-spacing: 0.5px;
}

