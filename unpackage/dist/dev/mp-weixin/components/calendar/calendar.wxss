
.all {
		width:calc(670rpx - 108rpx);
		height:calc(672rpx - 108rpx);
		background:rgba(17,23,57,1);
		box-shadow:0px 9rpx 27rpx rgba(0,0,0,0.3);
		opacity:1;
		border-radius:27rpx;
		padding: 54rpx;
}
.all .bar {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		-webkit-box-pack: end;
		-webkit-justify-content: flex-end;
		        justify-content: flex-end;
}
.show-mounth{
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-align: end;
		-webkit-align-items: flex-end;
		        align-items: flex-end;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		margin-right: 11rpx;
}
.mounth{
		font-size:12px;
		font-family:Alibaba PuHuiTi;
		font-weight:500;
		line-height:38rpx;
		color:rgba(107,119,141,1);
		opacity:1;
}
.mounth-en{
		font-size:15px;
		font-family:Alibaba PuHuiTi;
		font-weight:500;
		line-height:38rpx;
		color:rgba(255,255,255,1);
		opacity:1;
}
.line{
		width:7rpx;
		height: 90rpx;
}
.all .week {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		-webkit-justify-content: space-around;
		        justify-content: space-around;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		width:562rpx;
		height:69rpx;
		background:rgba(39,45,76,1);
		opacity:1;
		border-radius:9rpx;
		font-size: 14px;
		font-family:Alibaba PuHuiTi;
		font-weight:500;
		color:rgba(133,137,151,1);
		opacity:1;
		margin: 24rpx 0 9rpx 0;
}
.myDateTable {
		width: 100%;
		height: auto;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		-webkit-flex-wrap: wrap;
		        flex-wrap: wrap;
}
.myDateTable .dateCell {
			width:72rpx;
			height:72rpx;
			background:rgba(244,99,122,0.2);
			text-align: center;
			line-height: 72rpx;
			font-size: 14px;
			color:rgba(133,137,151,1);
			margin-bottom: 9rpx;
			border-radius: 9rpx;
}
.dateCell .cell {
			border-radius: 9rpx;
}
.whiteColor {
		color:rgba(133,137,151,1);
}
.greenColor {
		color: #FFFFFF;
		font-weight: bold;
}
.bgWhite {
		background:rgba(244,99,122,1);
}
	
	

